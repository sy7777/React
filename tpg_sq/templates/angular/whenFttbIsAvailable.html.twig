<div ng-if="view === 'whenFttbIsAvailable'">
    <h2 class="coloured-title">
        Congratulations
    </h2>
    <div class="technology-container">
        <h2 class="sq-result-title fa-check-circle">
            <span ng-if="sq.FTTB.res === 1 && sq.FTTB.technology !== 'FTTH'">{* productInfo.FTTB.name *}</span>
            <span ng-if="sq.FTTB.res === 1 && sq.FTTB.technology === 'FTTH'">{* productInfo.FTTH.name *}</span>
            is available at your address
        </h2>
    </div>
    <div class="text-center">
        <span class="sqAddress">{* sq.formed_fulladdress *}</span>
        <a href="#" class="pl-10" ng-click="resetSq()">Change address</a>
    </div>
</div>
